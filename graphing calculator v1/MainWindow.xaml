<Window x:Class="graphing_calculator_v1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:graphing_calculator_v1"
        mc:Ignorable="d"
        Title="graphing calclator" Height="450" Width="800"
        Loaded="WindowLoaded"
        MouseWheel="Window_MouseWheel"
        KeyDown="Window_KeyDown"
        KeyUp="Window_KeyUp">
    <Grid>
        <Canvas Name="drawboard" Background="White" Focusable="True"></Canvas>
        <Label Content="Label" HorizontalAlignment="Left" VerticalAlignment="Bottom" Name ="scalename"/>
        <TextBox HorizontalAlignment="Left" TextWrapping="Wrap" Text="x*x" FontSize="20" TextAlignment="Left" VerticalAlignment="Top" Width="150" Height="34" Margin="0,10,0,0" KeyDown="TextBox_KeyDown" Name="input" />
        <Border Name="hiddenFocusTarget" Focusable="True" Visibility="Collapsed"/>
        
    </Grid>
</Window>
